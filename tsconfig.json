{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "target": "ES2021",
    "module": "ESNext",
    "outDir": "dist",
    "esModuleInterop": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true
  }
}
