{
  "name": "Enrolment",
  "attributes": {
    "StudentId": { "type": "String", "maxLength": 100, "flags": "KMI-" },
    "CourseName": { "type": "String", "maxLength": 20, "flags": "KMI-" }
  },
  "relationships": {
    "StudentRef": {
      "model": "Student",
      "relationship": "ONE_TO_MANY",
      "mapping": {
        "from": ["StudentId"],
        "to": ["Id"]
      },
      "onDelete": "CASCADE"
    },
    "CourseRef": {
      "model": "Course",
      "relationship": "ONE_TO_MANY",
      "mapping": {
        "from": ["CourseName"],
        "to": ["CourseName"]
      },
      "onDelete": "CASCADE"
    }
  }
}
